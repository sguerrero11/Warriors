<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Smoke" >
    <listeners>
        <listener class-name="utils.ProjectListener"/>
    </listeners>
    <test name="Smoke" group-by-instances="true">
        <groups>
            <run>
                <include name="Smoke"/>
                <exclude name="Skipped" />
            </run>
        </groups>
        <classes>
            <class name= "FrontEnd.LoadContactUsForm"/>

        </classes>
    </test>
</suite>

